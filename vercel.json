{
  "version": 2,
  "buildCommand": "cd client && chmod +x ./node_modules/.bin/vite && npm run build",
  "installCommand": "cd client && pnpm install --filter ./client --no-frozen-lockfile",
  "outputDirectory": "client/dist",
  "framework": "vite",
  "rewrites": [
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
